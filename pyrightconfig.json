{
  "include": [
    "nebulus_atom",
    "nebulus_swarm"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.pytest_cache",
    "**/.mypy_cache",
    "**/venv",
    "**/.venv",
    "**/_build",
    "**/dist",
    "**/build"
  ],
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.12",
  "pythonPlatform": "Linux",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.12",
      "extraPaths": [
        "nebulus_atom",
        "nebulus_swarm"
      ]
    }
  ],
  "venvPath": ".",
  "venv": "venv"
}
